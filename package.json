{
  "name": "cuddy",
  "description": "An aggregation pipeline built in a functional programming style that lets you search, group, order and transform a collection.",
  "version": "0.3.0-beta.0",
  "author": "Bogdan Lazar",
  "exports": {
    ".": "./dist/cuddy.js",
    "./helpers": "./dist/helpers.js",
    "./package.json": "./package.json"
  },
  "typings": "dist/cuddy.d.ts",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">= 16.13",
    "npm": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tricinel/cuddy.git"
  },
  "bugs": {
    "url": "https://github.com/tricinel/cuddy/issues"
  },
  "homepage": "https://github.com/tricinel/cuddy#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "lint": "eslint src",
    "prepublishOnly": "npm-run-all test typecheck lint build"
  },
  "dependencies": {
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/ramda": "^0.27.46",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-jest": "^27.3.1",
    "esbuild": "^0.13.9",
    "eslint": "^8.1.0",
    "eslint-config-frontwerk-typescript": "^1.2.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.6.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "search",
